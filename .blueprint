    - database/factories/CityFactory.php
    - database/factories/LocationFactory.php
    - database/factories/RoomFactory.php
    - database/factories/TerminationFactory.php
    - database/migrations/2024_01_29_162938_create_regions_table.php
    - database/migrations/2024_01_29_162939_create_cities_table.php
    - database/migrations/2024_01_29_162940_create_locations_table.php
    - database/migrations/2024_01_29_162941_create_rooms_table.php
    - database/migrations/2024_01_29_162942_create_terminations_table.php
