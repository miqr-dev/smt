models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string:100 nullable', guid: 'string nullable', domain: 'string nullable' }
    Region: { name: string, timestamps: timestamps, softdeletes: softDeletes, relationships: { hasMany: City } }
    City: { name: string, timestamps: timestamps, softdeletes: softDeletes, relationships: { hasMany: 'User, Location', belongsTo: Region } }
    Location: { name: string, city_id: 'id:city', timestamps: timestamps, softdeletes: softDeletes, relationships: { belongsTo: City, hasMany: Room } }
    Room: { name: string, alt_name: string, floor: string, timestamps: timestamps, softdeletes: softDeletes, relationships: { belongsTo: Location } }
    Termination: {note: text, exit: date, softdeletes: softDeletes, timestamps: timestamps, relationships: { belongsTo: User } }
